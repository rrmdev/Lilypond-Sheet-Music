vb = \relative c' {
  \voiceconsts

  \repeat volta 2 {
    f4 e d d
    d(cis) d4.(\fermata e8)
    f4 a8( gis) a4 a8( g)
    f4( g8 b) a2\fermata

    f4 b b a8( g)
    f1\fermata
  }

  \repeat volta 2 {
    f4 f f f
    f8( d) e4 f2\fermata

    f4 gis a a8( g)
    f4 e f( gis)
    a2\fermata f4 e \rall
    d d d ( cis)
    d1\fermata
  }
}