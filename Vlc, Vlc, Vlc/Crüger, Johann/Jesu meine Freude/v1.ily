va = \relative c'' {
  \voiceconsts

  \repeat volta 2 {
    a4 a g f
    e2 d\fermata
    a'4 h c a
    d2 cis\fermata

    d8( e) f4 e e
    d1\fermata
  }

  \repeat volta 2 {
    a4 a b? a
    g4. g8 f2\fermata

    a4 h c a
    d c8( h) h2
