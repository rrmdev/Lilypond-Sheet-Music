vc = \relative c' {
  \voiceconsts

  \repeat volta 2 {
    d4 c b a
    g( a) d2\fermata
    d8( f) e( d) c( a) f'( e)
    d([ c b g]) a2\fermata

    b8( a) g4 a h8( cis)
    d1\fermata
  }

  \repeat volta 2 {
    f4 e d c?
    b? c f(\fermata e)

    d c8( h) a( g') f( e)
    d4 a d( e8 d)
    cis2\fermata d4 c \rall
    b? a gis( a)
    d1\fermata
  }
}