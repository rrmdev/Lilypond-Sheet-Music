vb = \relative c {
  \voiceconsts
  
  \repeat volta 2 {
    e16(\mf f) e( f) r8 e16( f)
    e( f) e( f) r8 e16( f)
    e( f) e( f) r8 e16( f)

    d( e) f8 \fine e4
  }

  \repeat volta 2 {
    gis16( a h8) gis16( a h8)
    c16( h a8) f16( gis a8)

    gis16( a h8) h4
    c16( h a8) a4
    gis16( a) gis( a) a( gis) f8

    e16( f) e( f) f( e) d8
    e( f) e( f) \dcaf
    e16( gis8.~ gis4)
  }
}
