va = \relative c {
  \voiceconsts
  
  \repeat volta 2 {
    e8\mf e e16( f) gis8
    e e e16( f) gis8
    h16( a) a( gis) gis( f) f( e)

    gis8 f \fine e4
  }

  \repeat volta 2 {
    gis16( a h8) h4
    c16( h a8) a4

    gis16( a h8) h4
    c16( h a8) a4
    gis16( a) h8 a16( gis) f8

    e16( f) gis8 f16( e d8)
    e( f) e( f) \dcaf
    e16( gis8.~ gis4)
  }
}
