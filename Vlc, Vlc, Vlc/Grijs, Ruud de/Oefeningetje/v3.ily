  \repeat volta 2 {
    c4. e8~ e4 c
    h4. d8~ d4 h4
    a4. c8~ c4 a
    f4. g8~ g4 a
    h4. c8~ c4 d4
    c4. e8~ e4 c
    h4. d8~ d4 h4
    
    a4. c8~ c4 a
    f4. g8~ g4 a
    h4. c8~ c4 d4 \bar "||" \time 3/4
    e4. c8 r g
    d'4. h8 r g
    c4. a8 r e \bar "||" \time 4/4
    f4. c'8~ c4 h4 \bar "||" \time 3/4

    e4. c8 r g
    d'4. h8 r g
    c4. a8 r e \bar "||" \time 4/4
