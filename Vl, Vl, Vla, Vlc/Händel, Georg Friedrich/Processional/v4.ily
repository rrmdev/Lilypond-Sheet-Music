vd = \relative c {
  \voiceconsts
  \clef "bass"

  \repeat volta 2 {
    d4 d cis
    d a h
    fis' fis fis
    h,2.
    e4 g, e'
    a cis, a'
    d, d d
    d fis a
  }
  \repeat volta 2 {
    d, d d
    cis cis cis
    h h h
    fis'2 d4
    g h, cis
    d fis h,

    a a a
    a cis a
    d d fis
    a, a2
    d4 d fis

    a, a2
    d4 d a
    d fis a,
    d d d
    d2.
  }
}