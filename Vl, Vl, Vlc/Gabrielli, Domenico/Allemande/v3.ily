vc = \relative c' {
  \voiceconsts
  \clef "bass"

  \repeat volta 2 {
    \partial 8*5 ais8\downbow h4. g8
    fis4 r8 d e fis g4
    fis4. fis8 h, h' gis cis,

    fis4 d8 cis h4 cis
    fis,4.
  }
  
  \repeat volta 2 {
    ais'8 h e, a a,
    d4 h'8 a g? d a' a,
    d4 r8 d' cis4 fis,
    h fis e8 h fis' fis,
    h4.
  }  
}