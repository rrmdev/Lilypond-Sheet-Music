vb = \relative c {
  \voiceconsts

  \repeat volta 2 {
    fis4 d \times 2/3 { g8( a h) }
    h4 a g
    fis8. g16 fis4 e
    e d a

    d r2
    a g'8. fis16
    fis4 e8[ d cis d]
    a4 a8[ g fis e]
    d4 d' \times 2/3 { g8 a h }
    h4 a g

    fis8. g16 fis4 e
    e( d) a
    d r2
    cis4 g' fis
    \times 2/3 { g8 fis e } d4 a
    d a d,
  }

  \repeat volta 2 {
    d' d d
    cis cis cis
    h e8[ fis gis a]
    h[ e, fis gis] a4
    e2.
    h'8[ e, a e gis e]

    a4 e e,
    a8[ h cis d] e g
    fis4 d \times 2/3 { g8( a h) }
    h4 a g
    fis8. g16 fis4 e
    e d8[ a cis a]

    d2 d8 cis
    cis4 g' fis
    \times 2/3 { g8( fis e) } d4 a
    d a d,
  }
}