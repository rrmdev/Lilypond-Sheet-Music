vb = \relative c' {
  \voiceconsts

  \repeat volta 2 {
    r4 <g c>_\pizz c, <g' c>
    r <g c> c, <g' c>
    r <g d'> g, <g' d'>
    r <g c> c, <g' c>
    
    r <g c> c, <g' c>
    r <g c> c, <g' c>
    r <g d'> g, <g' d'>
    r <g c> c, <g' c>
  }

  \repeat volta 2 {
    r <g c> c, <g' c>

    r <g c> c, <g' c>
    r <g d'> g, <g' d'>
    r <g c> c, <g' c>
    r <g c> c, <g' c>
    r <g c> c, <g' c>

    <f c'> <g c> g, <g' c>
    r <g c> c, <g' c>
  }
  <f c'> <g c> g, <g' c>
  r <g c> c, <g' c>
  R1 \bar "|."
}